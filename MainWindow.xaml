            <TextBlock Text="Среднее время работы" Margin="5"/>
            <TextBox Margin="5"/>
            <TextBlock Text="Среднее отклонение" Margin="5"/>
            <TextBox Margin="5"/>
            <TextBlock Text="Время моделирования" Margin="5"/>
            <TextBox Margin="5"/>
            
        </StackPanel>
        </DockPanel>
        
        
    </Grid>
</Window>
